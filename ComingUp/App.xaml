<Application
    x:Class="ComingUp.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    RequestedTheme="Light">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<!--Templates-->
				<ResourceDictionary Source="Templates/AutoSuggestBoxTemplate.xaml" />
				<ResourceDictionary Source="Templates/SavedMediaFlipViewDataTemplate.xaml" />

				<!--Styles-->
				<ResourceDictionary Source="Styles/DefaultStyles.xaml" />
				<ResourceDictionary Source="Styles/SplitViewStyles.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>